<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="drawing.demo.section.pkg3.FXMLDocumentController">
   <children>
      <VBox>
         <children>
            <Canvas fx:id="drawingCanvas" height="600.0" layoutX="14.0" layoutY="14.0" width="750.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <ToolBar prefHeight="40.0" prefWidth="200.0">
               <items>
                  <ToggleButton fx:id="gridToggle" mnemonicParsing="false" onAction="#handleNFieldAction" text="Grid" />
                  <TextField fx:id="nField" onAction="#handleNFieldAction" onKeyReleased="#handleNFieldKeyAction" prefHeight="25.0" prefWidth="56.0" />
                  <Slider />
                  <ToggleButton fx:id="filledPolygonToggle" mnemonicParsing="false" onAction="#handleNFieldAction" text="Filled Polygon" />
               </items>
            </ToolBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
